<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.xupt.crm.mapper.BaseDictMapper">
	
	<select id="selectBaseDictByCodeId" parameterType="String" resultType="com.xupt.crm.pojo.BaseDict">
		SELECT * FROM base_dict WHERE dict_type_code =
		#{dict_type_code}
	
	</select>

</mapper>